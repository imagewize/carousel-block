{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "cb/carousel",
  "version": "1.0.0",
  "title": "Carousel",
  "category": "design",
  "description": "Create a customizable slideshow of content with this carousel block.",
  "attributes": {
    "slidesToShow": {
      "type": "number",
      "default": 1
    },
    "slidesToScroll": {
      "type": "number",
      "default": 1
    },
    "arrows": {
      "type": "boolean",
      "default": true
    },
    "dots": {
      "type": "boolean",
      "default": false
    },
    "infinite": {
      "type": "boolean",
      "default": true
    },
    "autoplay": {
      "type": "boolean",
      "default": false
    },
    "autoplaySpeed": {
      "type": "number",
      "default": 3000
    },
    "speed": {
      "type": "number",
      "default": 500
    },
    "rtl": {
      "type": "boolean",
      "default": false
    }
  },
  "supports": {
    "align": [
      "wide",
      "full"
    ],
    "html": false,
    "anchor": true
  },
  "textdomain": "carousel-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./editor.css",
  "style": "file:./style.scss"
}